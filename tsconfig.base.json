{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "@energinet-datahub/dh/core/shell-core": [
        "libs/dh/core/shell-core/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/data-access-api": [
        "libs/ett/auth/data-access-api/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/data-access-security": [
        "libs/ett/auth/data-access-security/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/feature-login": [
        "libs/ett/auth/feature-login/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/routing-security": [
        "libs/ett/auth/routing-security/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/shell-auth": [
        "libs/ett/auth/shell-auth/src/index.ts"
      ],
      "@energinet-datahub/ett/core/environments": [
        "libs/ett/core/environments/src/index.ts"
      ],
      "@energinet-datahub/ett/core/shell": [
        "libs/ett/core/shell/src/index.ts"
      ],
      "@energinet-datahub/ett/dashboard/routing": [
        "libs/ett/dashboard/routing/src/index.ts"
      ],
      "@energinet-datahub/ett/dashboard/shell": [
        "libs/ett/dashboard/shell/src/index.ts"
      ],
      "@energinet-datahub/ett/shared/util-browser": [
        "libs/ett/shared/util-browser/src/index.ts"
      ],
      "@energinet-datahub/gf/test-util-matchers": [
        "libs/gf/test-util-matchers/src/index.ts"
      ],
      "@energinet-datahub/gf/test-util-staging": [
        "libs/gf/test-util-staging/src/index.ts"
      ],
      "@energinet-datahub/gf/util-browser": [
        "libs/gf/util-browser/src/index.ts"
      ],
      "@energinet-datahub/watt": [
        "libs/ui-watt/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
