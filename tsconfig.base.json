{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular/*": ["./node_modules/@angular/*"],
      "@energinet-datahub/dh-app/core/feature-shell": [
        "libs/dh-app/core/feature-shell/src/index.ts"
      ],
      "@energinet-datahub/dh-app/shared/assets": [
        "libs/dh-app/shared/assets/src/index.ts"
      ],
      "@energinet-datahub/dh-app/shared/environments": [
        "libs/dh-app/shared/environments/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/data-access-api": [
        "libs/ett/auth/data-access-api/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/data-access-security": [
        "libs/ett/auth/data-access-security/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/feature-login": [
        "libs/ett/auth/feature-login/src/index.ts"
      ],
      "@energinet-datahub/ett/auth/feature-shell": [
        "libs/ett/auth/feature-shell/src/index.ts"
      ],
      "@energinet-datahub/ett/core/environments": [
        "libs/ett/core/environments/src/index.ts"
      ],
      "@energinet-datahub/ett/core/feature-shell": [
        "libs/ett/core/feature-shell/src/index.ts"
      ],
      "@energinet-datahub/ett/core/util-browser": [
        "libs/ett/core/util-browser/src/index.ts"
      ],
      "@energinet-datahub/ett/dashboard/feature-shell": [
        "libs/ett/dashboard/feature-shell/src/index.ts"
      ],
      "@energinet-datahub/gf/test-util-matchers": [
        "libs/gf/test-util-matchers/src/index.ts"
      ],
      "@energinet-datahub/gf/test-util-staging": [
        "libs/gf/test-util-staging/src/index.ts"
      ],
      "@energinet-datahub/gf/util-browser": [
        "libs/gf/util-browser/src/index.ts"
      ],
      "@energinet-datahub/watt": ["libs/ui-watt/src/index.ts"]
