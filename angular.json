{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "dh-app",
  "projects": {
    "dh-app": {
      "projectType": "application",
      "root": "apps/dh-app",
      "sourceRoot": "apps/dh-app/src",
      "prefix": "dh",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dh-app",
            "index": "apps/dh-app/src/index.html",
            "main": "apps/dh-app/src/main.ts",
            "polyfills": "apps/dh-app/src/polyfills.ts",
            "tsConfig": "apps/dh-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/dh-app/src/favicon.ico", "apps/dh-app/src/assets"],
            "styles": ["apps/dh-app/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui-watt/src/lib/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/dh-app/src/environments/environment.ts",
                  "with": "apps/dh-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "dh-app:build:production"
            },
            "development": {
              "browserTarget": "dh-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dh-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dh-app/src/**/*.ts",
              "apps/dh-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dh-app"],
          "options": {
            "jestConfig": "apps/dh-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dh-app-core-feature-shell": {
      "projectType": "library",
      "root": "libs/dh-app/core/feature-shell",
      "sourceRoot": "libs/dh-app/core/feature-shell/src",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dh-app/core/feature-shell"],
          "options": {
            "jestConfig": "libs/dh-app/core/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/dh-app/core/feature-shell/src/**/*.ts",
              "libs/dh-app/core/feature-shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "dh-app-e2e": {
      "root": "apps/dh-app-e2e",
      "sourceRoot": "apps/dh-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dh-app-e2e/cypress.json",
            "tsConfig": "apps/dh-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "dh-app:serve:development"
          },
          "configurations": {
            "ci": {
              "cypressConfig": "apps/dh-app-e2e/cypress.ci.json",
              "headless": true
            },
            "ci:production": {
              "cypressConfig": "apps/dh-app-e2e/cypress.ci.json",
              "devServerTarget": "dh-app:serve:production",
              "headless": true
            },
            "production": {
              "devServerTarget": "dh-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dh-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ett-app": {
      "projectType": "application",
      "root": "apps/ett-app",
      "sourceRoot": "apps/ett-app/src",
      "prefix": "ett",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "assets": [
              "apps/ett-app/src/favicon.ico",
              "apps/ett-app/src/assets"
            ],
            "baseHref": "/",
            "commonChunk": false,
            "index": "apps/ett-app/src/index.html",
            "inlineStyleLanguage": "scss",
            "main": "apps/ett-app/src/main.ts",
            "outputPath": "dist/apps/ett-app",
            "polyfills": "apps/ett-app/src/polyfills.ts",
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui-watt/src/lib/styles"]
            },
            "styles": ["apps/ett-app/src/styles.scss"],
            "tsConfig": "apps/ett-app/tsconfig.app.json"
          },
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "budgets": [
                {
                  "type": "allScript",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyScript",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "bundle",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "libs/ett/core/environments/src/lib/environment.ts",
                  "with": "libs/ett/core/environments/src/lib/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "ett-app:build:development",
              "hmr": true,
              "proxyConfig": "apps/ett-app/src/proxy.conf.json"
            },
            "production": {
              "browserTarget": "ett-app:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ett-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ett-app/src/**/*.ts",
              "apps/ett-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ett-app"],
          "options": {
            "jestConfig": "apps/ett-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ett-app-e2e": {
      "root": "apps/ett-app-e2e",
      "sourceRoot": "apps/ett-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ett-app-e2e/cypress.json",
            "tsConfig": "apps/ett-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "ett-app:serve:development"
          },
          "configurations": {
            "ci": {
              "cypressConfig": "apps/ett-app-e2e/cypress.ci.json",
              "headless": true
            },
            "ci:production": {
              "cypressConfig": "apps/ett-app-e2e/cypress.ci.json",
              "devServerTarget": "ett-app:serve:production",
              "headless": true
            },
            "production": {
              "devServerTarget": "ett-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ett-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ett-auth-feature-login": {
      "projectType": "library",
      "root": "libs/ett/auth/feature-login",
      "sourceRoot": "libs/ett/auth/feature-login/src",
      "prefix": "ett",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/auth/feature-login"],
          "options": {
            "jestConfig": "libs/ett/auth/feature-login/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/auth/feature-login/src/**/*.ts",
              "libs/ett/auth/feature-login/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-auth-feature-shell": {
      "projectType": "library",
      "root": "libs/ett/auth/feature-shell",
      "sourceRoot": "libs/ett/auth/feature-shell/src",
      "prefix": "ett",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/auth/feature-shell"],
          "options": {
            "jestConfig": "libs/ett/auth/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/auth/feature-shell/src/**/*.ts",
              "libs/ett/auth/feature-shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-core-data-access-auth": {
      "projectType": "library",
      "root": "libs/ett/core/data-access-auth",
      "sourceRoot": "libs/ett/core/data-access-auth/src",
      "prefix": "energinet-datahub",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/core/data-access-auth"],
          "options": {
            "jestConfig": "libs/ett/core/data-access-auth/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/core/data-access-auth/src/**/*.ts",
              "libs/ett/core/data-access-auth/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-core-environments": {
      "projectType": "library",
      "root": "libs/ett/core/environments",
      "sourceRoot": "libs/ett/core/environments/src",
      "prefix": "ett",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/core/environments"],
          "options": {
            "jestConfig": "libs/ett/core/environments/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/core/environments/src/**/*.ts",
              "libs/ett/core/environments/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-core-feature-shell": {
      "projectType": "library",
      "root": "libs/ett/core/feature-shell",
      "sourceRoot": "libs/ett/core/feature-shell/src",
      "prefix": "ett",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/core/feature-shell"],
          "options": {
            "jestConfig": "libs/ett/core/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/core/feature-shell/src/**/*.ts",
              "libs/ett/core/feature-shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-core-util-browser": {
      "projectType": "library",
      "root": "libs/ett/core/util-browser",
      "sourceRoot": "libs/ett/core/util-browser/src",
      "prefix": "ett",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/core/util-browser"],
          "options": {
            "jestConfig": "libs/ett/core/util-browser/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/core/util-browser/src/**/*.ts",
              "libs/ett/core/util-browser/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-dashboard-feature-shell": {
      "projectType": "library",
      "root": "libs/ett/dashboard/feature-shell",
      "sourceRoot": "libs/ett/dashboard/feature-shell/src",
      "prefix": "ett-dashboard",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/dashboard/feature-shell"],
          "options": {
            "jestConfig": "libs/ett/dashboard/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/dashboard/feature-shell/src/**/*.ts",
              "libs/ett/dashboard/feature-shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "ett-shared-test-util-staging": {
      "projectType": "library",
      "root": "libs/ett/shared/test-util-staging",
      "sourceRoot": "libs/ett/shared/test-util-staging/src",
      "prefix": "ett-test",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ett/shared/test-util-staging"],
          "options": {
            "jestConfig": "libs/ett/shared/test-util-staging/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ett/shared/test-util-staging/src/**/*.ts",
              "libs/ett/shared/test-util-staging/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui-watt": {
      "projectType": "library",
      "root": "libs/ui-watt",
      "sourceRoot": "libs/ui-watt/src",
      "prefix": "watt",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-watt"],
          "options": {
            "jestConfig": "libs/ui-watt/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-watt/src/**/*.ts",
              "libs/ui-watt/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-watt/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/ui-watt",
            "config": {
              "configFolder": "libs/ui-watt/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "workspace-tools": {
      "projectType": "tool",
      "root": "tools",
      "sourceRoot": "tools",
      "architect": {
        "add-license": {
          "builder": "./tools/executors/license:add"
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["tools/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/tools"],
          "options": {
            "jestConfig": "tools/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
