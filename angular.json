            "cypressConfig": "apps/ett/e2e-ett/cypress.json",
            "tsConfig": "apps/ett/e2e-ett/tsconfig.e2e.json",
            "devServerTarget": "app-ett:serve:e2e"
          },
          "configurations": {
            "ci": {
              "cypressConfig": "apps/ett/e2e-ett/cypress.ci.json",
              "headless": true
            },
            "ci:production": {
              "cypressConfig": "apps/ett/e2e-ett/cypress.ci.json",
