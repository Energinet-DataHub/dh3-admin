{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "app-dh",
  "projects": {
    "app-dh": {
      "projectType": "application",
      "root": "apps/dh/app-dh",
      "sourceRoot": "apps/dh/app-dh/src",
      "prefix": "dh",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dh/app-dh",
            "index": "apps/dh/app-dh/src/index.html",
            "main": "apps/dh/app-dh/src/main.ts",
            "polyfills": "apps/dh/app-dh/src/polyfills.ts",
            "tsConfig": "apps/dh/app-dh/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/dh/app-dh/src/favicon.ico",
              "apps/dh/app-dh/src/assets",
              {
                "input": "libs/ui-watt/src/assets",
                "glob": "**/*",
                "output": "assets/ui-watt"
              }
            ],
            "styles": ["apps/dh/app-dh/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui-watt/src/lib/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/dh/app-dh/src/environments/environment.ts",
                  "with": "apps/dh/app-dh/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app-dh:build:production"
            },
            "development": {
              "browserTarget": "app-dh:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-dh:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dh/app-dh/src/**/*.ts",
              "apps/dh/app-dh/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dh/app-dh"],
          "options": {
            "jestConfig": "apps/dh/app-dh/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-dh-core-feature-shell": {
      "projectType": "library",
      "root": "libs/dh/core/feature-shell",
      "sourceRoot": "libs/dh/core/feature-shell/src",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dh/core/feature-shell"],
          "options": {
            "jestConfig": "libs/dh/core/feature-shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/dh/core/feature-shell/src/**/*.ts",
              "libs/dh/core/feature-shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "app-dh-e2e": {
      "root": "apps/dh/e2e-dh",
      "sourceRoot": "apps/dh/e2e-dh/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dh/e2e-dh/cypress.json",
            "tsConfig": "apps/dh/e2e-dh/tsconfig.e2e.json",
            "devServerTarget": "app-dh:serve:development"
          },
          "configurations": {
            "ci": {
              "cypressConfig": "apps/dh/e2e-dh/cypress.ci.json",
              "headless": true
            },
            "ci:production": {
              "cypressConfig": "apps/dh/e2e-dh/cypress.ci.json",
              "devServerTarget": "app-dh:serve:production",
              "headless": true
            },
            "production": {
              "devServerTarget": "app-dh:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dh/e2e-dh/**/*.{js,ts}"]
          }
        }
      }
    },
    "app-ett": {
      "projectType": "application",
      "root": "apps/ett/app-ett",
      "sourceRoot": "apps/ett/app-ett/src",
      "prefix": "ett",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "assets": [
              "apps/ett/app-ett/src/favicon.ico",
              "apps/ett/app-ett/src/assets"
            ],
            "baseHref": "/",
            "commonChunk": false,
            "index": "apps/ett/app-ett/src/index.html",
            "inlineStyleLanguage": "scss",
            "main": "apps/ett/app-ett/src/main.ts",
            "outputPath": "dist/apps/ett/app-ett",
            "polyfills": "apps/ett/app-ett/src/polyfills.ts",
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui-watt/src/lib/styles"]
            },
            "styles": ["apps/ett/app-ett/src/styles.scss"],
            "tsConfig": "apps/ett/app-ett/tsconfig.app.json"
          },
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "budgets": [
                {
                  "type": "allScript",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyScript",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "bundle",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "libs/ett/core/environments/src/lib/environment.ts",
                  "with": "libs/ett/core/environments/src/lib/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "app-ett:build:development",
              "hmr": true,
              "proxyConfig": "apps/ett/app-ett/src/proxy.conf.json"
            },
            "e2e": {
              "browserTarget": "app-ett:build:development"
            },
            "production": {
              "browserTarget": "app-ett:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-ett:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ett/app-ett/src/**/*.ts",
              "apps/ett/app-ett/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ett/app-ett"],
          "options": {
            "jestConfig": "apps/ett/app-ett/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-ett-e2e": {
      "root": "apps/ett/e2e-ett",
      "sourceRoot": "apps/ett/e2e-ett/src",
      "projectType": "application",
      "architect": {
        "e2e": {
