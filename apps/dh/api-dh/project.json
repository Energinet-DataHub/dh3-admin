{
  "projectType": "application",
  "root": "apps/dh/api-dh",
  "sourceRoot": "apps/dh/api-dh/source",
  "prefix": "dh",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npm run check:dotnet",
          "dotnet build apps/dh/api-dh/DataHub.WebApi.sln --configuration=Release"
        ],
        "outputPath": "apps/dh/api-dh/source/DataHub.WebApi/bin/Release/net5.0",
        "parallel": false
      },
      "outputs": ["{options.outputPath}"]
    },
    "build-client": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npm run check:java",
          "openapi-generator-cli generate -o libs/dh/shared/data-access-api/src/lib/generated/v1 -i apps/dh/api-dh/source/DataHub.WebApi/bin/Release/net5.0/swagger.json -g typescript-angular -p=fileNaming=kebab-case -p=serviceFileSuffix=-http.service -p=serviceSuffix=Http -p=supportsES6=true -p=taggedUnions=true",
          "rimraf libs/dh/shared/data-access-api/src/lib/generated/v1/.openapi-generator/",
          "rimraf libs/dh/shared/data-access-api/src/lib/generated/v1/.gitignore",
          "rimraf libs/dh/shared/data-access-api/src/lib/generated/v1/git_push.sh"
        ],
        "outputPath": "libs/dh/shared/data-access-api/src/lib/generated/v1",
        "parallel": false
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npm run check:dotnet",
          "dotnet run --no-build --project apps/dh/api-dh/source/DataHub.WebApi/DataHub.WebApi.csproj --configuration=Debug"
        ],
        "parallel": false
      }
    }
  },
  "tags": ["product:dh", "type:api"]
}
