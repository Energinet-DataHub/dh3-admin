{
  "name": "app-eov",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/eov/eov/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/eov/eov",
        "index": "apps/eov/eov/src/index.html",
        "main": "apps/eov/eov/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/eov/eov/tsconfig.app.json",
        "assets": [
          {
            "glob": "favicon.ico",
            "input": "libs/eov/shared/assets/src",
            "output": ""
          },
          {
            "glob": "**/*",
            "input": "libs/eov/shared/assets/src/assets",
            "output": "assets"
          },
          {
            "input": "libs/watt/src/assets",
            "glob": "**/*",
            "output": "assets/watt"
          }
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "libs/watt/src/lib/styles"
          ]
        },
        "styles": ["apps/eov/eov/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "app-eov:build:production"
        },
        "development": {
          "browserTarget": "app-eov:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "app-eov:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/eov/eov/**/*.ts", "apps/eov/eov/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/eov/eov/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "app-eov:build"
      }
    }
  }
}
