{
  "root": "apps/dh-app-e2e",
  "sourceRoot": "apps/dh-app-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/dh-app-e2e/cypress.json",
        "tsConfig": "apps/dh-app-e2e/tsconfig.e2e.json",
        "devServerTarget": "dh-app:serve:development"
      },
      "configurations": {
        "ci": {
          "cypressConfig": "apps/dh-app-e2e/cypress.ci.json",
          "headless": true
        },
        "ci:production": {
          "cypressConfig": "apps/dh-app-e2e/cypress.ci.json",
          "devServerTarget": "dh-app:serve:production",
          "headless": true
        },
        "production": {
          "devServerTarget": "dh-app:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/dh-app-e2e/**/*.{js,ts}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["product:dh", "type:e2e"],
  "implicitDependencies": ["dh-app"]
}
