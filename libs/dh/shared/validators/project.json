    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/dh/shared/validators/**/*.ts",
          "libs/dh/shared/validators/**/*.html"
        ]
      }
    }
  },
  "tags": ["product:dh", "domain:shared", "type:ui"]
}
