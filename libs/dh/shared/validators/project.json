{
  "name": "dh-validators",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/dh/shared/validators/src",
  "prefix": "dh",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
