{
  "name": "dh-admin-domain",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dh/admin/domain/src",
  "prefix": "dh",
  "tags": ["product:dh", "domain:admin", "type:domain"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/dh/admin/domain/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/dh/admin/domain/**/*.ts",
          "libs/dh/admin/domain/**/*.html"
        ]
      }
    }
  }
}
