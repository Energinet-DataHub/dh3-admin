{
  "name": "energinet",
  "version": "0.0.0",
  "license": "MIT",
  "//engines-comment": "Keep this in sync with .nvmrc, and volta config in current package.json",
  "engines": {
    "node": "14.17.6",
    "npm": "Please use Yarn v1 instead of npm to install dependencies",
    "yarn": "1.22.11"
  },
  "scripts": {
    "postinstall": "node ./decorate-angular-cli.js && yarn ngcc",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build --configuration=production --parallel",
    "affected:ci": "yarn affected:lint && yarn affected:test && yarn affected:build && yarn affected:e2e",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e --parallel",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint --parallel",
    "affected:lint:fix": "nx affected:lint --fix --parallel",
    "affected:test": "nx affected:test --parallel",
    "build": "nx run-many --target=build --configuration=production --all --with-deps --parallel",
    "ci": "yarn lint && yarn test && yarn build && yarn e2e --configuration=ci",
    "designsystem:build": "yarn docs:json && npx nx run ui-watt:build-storybook --outputPath=dist/designsystem",
    "designsystem:start": "yarn docs:json && npx nx run ui-watt:storybook",
    "dep-graph": "nx dep-graph",
    "docs:json": "npx compodoc -p ./libs/ui-watt/tsconfig.json -e json -d .",
    "e2e": "nx run-many --target=e2e --all --with-deps --parallel",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "help": "nx help",
    "lint": "nx workspace-lint && nx run-many --target=lint --all --with-deps --parallel",
    "ng": "nx",
    "ngcc": "ngcc --first-only --properties es2015 module fesm2015 esm2015 browser main --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "test": "nx run-many --target=test --all --with-deps --parallel",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@ljcl/storybook-addon-cssprops": "^0.1.0",
    "@nrwl/angular": "12.7.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@compodoc/compodoc": "^1.1.14",
    "@nrwl/cli": "12.7.2",
    "@nrwl/cypress": "12.7.2",
    "@nrwl/eslint-plugin-nx": "12.7.2",
    "@nrwl/jest": "12.7.2",
    "@nrwl/linter": "12.7.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/storybook": "12.7.2",
    "@nrwl/tao": "12.7.2",
    "@nrwl/workspace": "12.7.2",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-storysource": "^6.3.7",
    "@storybook/angular": "~6.3.0",
    "@storybook/builder-webpack5": "~6.3.0",
    "@storybook/manager-webpack5": "~6.3.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.4",
    "ng-packagr": "^12.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  },
  "volta": {
    "//node-comment": "Keep this in sync with engines in current package.json",
    "node": "14.17.6",
    "yarn": "1.22.11"
  }
}
