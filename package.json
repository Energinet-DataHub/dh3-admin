{
  "name": "energinet-datahub",
  "version": "0.0.0",
  "license": "MIT",
  "//engines-comment": "Keep this in sync with .nvmrc, and volta config in current package.json",
  "engines": {
    "node": "14.18.1",
    "npm": "Please use Yarn v1 instead of npm to install dependencies",
    "yarn": "1.22.17"
  },
  "scripts": {
    "postinstall": "npm run postinstall:nx && npm run postinstall:ngcc",
    "postinstall:ngcc": "npm run ngcc",
    "postinstall:nx": "node ./decorate-angular-cli.js",
    "check:dotnet": "dotnet --version || echo ❌ dotnet CLI must be installed",
    "check:java": "java -version || echo ❌ Java must be installed",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build --configuration=production --parallel",
    "affected:ci": "npm run affected:lint && npm run affected:test && npm run affected:build && npm run affected:e2e -- --configuration=ci",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint --max-warnings=0 --parallel",
    "affected:lint:fix": "nx affected:lint --max-warnings=0 --fix --parallel",
    "affected:test": "nx affected:test --parallel",
    "build": "nx run-many --target=build --configuration=production --all --parallel",
    "ci": "npm run lint && npm run test && npm run build && npm run e2e -- --configuration=ci",
    "designsystem:build": "npm run docs:json && npm run nx -- run ui-watt:build-storybook --outputPath=dist/designsystem",
    "designsystem:start": "npm run docs:json && npm run nx -- run ui-watt:storybook",
    "dep-graph": "nx dep-graph",
    "docs:json": "npx compodoc -p ./libs/ui-watt/tsconfig.json -e json -d ./libs/ui-watt",
    "e2e": "nx run-many --target=e2e --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "help": "nx help",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "lint": "nx workspace-lint && nx run-many --target=lint --max-warnings=0 --all --parallel",
    "lint:fix": "nx workspace-lint && nx run-many --target=lint --max-warnings=0 --fix --all --parallel",
    "ng": "nx",
    "ngcc": "ngcc --properties es2015 module browser main",
    "ngcc:dh": "npm run ngcc -- --use-program-dependencies --tsconfig apps/dh/app-dh/tsconfig.app.json",
    "ngcc:ett": "npm run ngcc -- --use-program-dependencies --tsconfig apps/ett/app-ett/tsconfig.app.json",
    "nx": "nx",
    "start": "ng serve",
    "test": "nx run-many --target=test --all --parallel",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ngneat/transloco": "^3.0.7",
    "@ngrx/component-store": "~12.5.1",
    "@ngworker/router-component-store": "~0.0.1",
    "@nrwl/angular": "~13.1.3",
    "@rx-angular/cdk": ">=1.0.0-alpha.10 || ^1.0.0",
    "@rx-angular/template": ">=1.0.0-beta.28 || ^1.0.0",
    "include-media": "^1.4.10",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/language-service": "~12.2.0",
    "@compodoc/compodoc": "^1.1.14",
    "@ngneat/transloco-keys-manager": "^3.1.0",
    "@ngworker/spectacular": "~0.2.0",
    "@nrwl/cli": "13.1.3",
    "@nrwl/cypress": "13.1.3",
    "@nrwl/eslint-plugin-nx": "13.1.3",
    "@nrwl/jest": "13.1.3",
    "@nrwl/linter": "13.1.3",
    "@nrwl/nx-cloud": "12.5.2",
    "@nrwl/storybook": "13.1.3",
    "@nrwl/tao": "13.1.3",
    "@nrwl/workspace": "13.1.3",
    "@openapitools/openapi-generator-cli": "^2.4.14",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-storysource": "^6.3.7",
    "@storybook/angular": "~6.3.0",
    "@storybook/builder-webpack5": "~6.3.0",
    "@storybook/manager-webpack5": "~6.3.0",
    "@testing-library/angular": "^10.11.0",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@types/jest": "27.0.2",
    "@types/jest-dev-server": "^5.0.0",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-ngrx": "^1.46.4",
    "eslint-plugin-sonarjs": "~0.10.0",
    "glob": "^7.2.0",
    "jest": "27.2.3",
    "jest-dev-server": "^6.0.0",
    "jest-preset-angular": "10.0.1",
    "kill-port-process": "^3.0.1",
    "ng-mocks": "^12.5.0",
    "ng-packagr": "~12.2.5",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  },
  "volta": {
    "//node-comment": "Keep this in sync with engines in current package.json",
    "node": "14.18.1",
    "yarn": "1.22.17"
  }
}
