    "ts-jest": "27.1.4",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "volta": {
    "//node-comment": "Keep this in sync with engines in current package.json",
    "node": "16.15.1",
    "//yarn-comment": "Keep this in sync with github actions pipelines",
    "yarn": "1.22.19"
  }
}
