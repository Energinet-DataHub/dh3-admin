{
  "name": "energinet-datahub",
  "version": "0.0.0",
  "license": "MIT",
  "//engines-comment": "Keep this in sync with .nvmrc, and volta config in current package.json",
  "engines": {
    "node": "14.18.1",
    "npm": "Please use Yarn v1 instead of npm to install dependencies",
    "yarn": "1.22.17"
  },
  "scripts": {
    "postinstall": "npm run postinstall:nx && npm run postinstall:ngcc",
    "postinstall:ngcc": "npm run ngcc",
    "postinstall:nx": "node ./decorate-angular-cli.js",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build --configuration=production --parallel",
    "affected:ci": "npm run affected:lint && npm run affected:test && npm run affected:build && npm run affected:e2e",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint --max-warnings=0 --parallel",
    "affected:lint:fix": "nx affected:lint --max-warnings=0 --fix --parallel",
    "affected:test": "nx affected:test --parallel",
    "build": "nx run-many --target=build --configuration=production --all --parallel",
    "ci": "npm run lint && npm run test && npm run build && npm run e2e -- --configuration=ci",
    "designsystem:build": "yarn docs:json && yarn nx -- run ui-watt:build-storybook --outputPath=dist/designsystem",
    "designsystem:start": "yarn docs:json && yarn nx -- run ui-watt:storybook",
    "dep-graph": "nx dep-graph",
    "docs:json": "yarn compodoc -p ./libs/ui-watt/tsconfig.json -e json -d ./libs/ui-watt",
    "e2e": "nx run-many --target=e2e --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "help": "nx help",
    "lint": "nx workspace-lint && nx run-many --target=lint --max-warnings=0 --all --parallel",
    "lint:fix": "nx workspace-lint && nx run-many --target=lint --max-warnings=0 --fix --all --parallel",
    "ng": "nx",
    "ngcc": "ngcc --properties es2015 module browser main",
    "ngcc:dh": "npm run ngcc -- --use-program-dependencies --tsconfig apps/dh-app/tsconfig.app.json",
    "ngcc:ett": "npm run ngcc -- --use-program-dependencies --tsconfig apps/ett-app/tsconfig.app.json",
    "nx": "nx",
    "start": "ng serve",
    "test": "nx run-many --target=test --all --parallel",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@ngrx/component-store": "^12.4.0",
    "@nrwl/angular": "12.7.2",
    "@rx-angular/cdk": "^1.0.0-alpha.10",
    "@rx-angular/template": "^1.0.0-beta.28",
    "include-media": "^1.4.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@compodoc/compodoc": "^1.1.14",
    "@nrwl/cli": "12.7.2",
    "@nrwl/cypress": "12.7.2",
    "@nrwl/eslint-plugin-nx": "12.7.2",
    "@nrwl/jest": "12.7.2",
    "@nrwl/linter": "12.7.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/storybook": "12.7.2",
    "@nrwl/tao": "12.7.2",
    "@nrwl/workspace": "12.7.2",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-storysource": "^6.3.7",
    "@storybook/angular": "~6.3.0",
    "@storybook/builder-webpack5": "~6.3.0",
    "@storybook/manager-webpack5": "~6.3.0",
    "@testing-library/angular": "^10.11.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "chromatic": "^6.0.4",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-ngrx": "^1.46.0",
    "glob": "^7.2.0",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.4",
    "ng-mocks": "^12.5.0",
    "ng-packagr": "^12.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  },
  "volta": {
    "//node-comment": "Keep this in sync with engines in current package.json",
    "node": "14.18.1",
    "yarn": "1.22.17"
  }
}
