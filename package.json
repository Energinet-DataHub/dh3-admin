  "//engines-comment": "Keep this in sync with '.nvmrc', and 'volta' config at the bottom of this file.",
  "engines": {
    "node": "16.18.0",
    "npm": "Please use Yarn v1 instead of npm to install dependencies.",
    "yarn": "1.22.19"
  },
  "scripts": {
    "postinstall": "npm run postinstall:nx && npm run postinstall:ngcc && npm run postinstall:husky",
    "postinstall:ngcc": "npm run ngcc",
    "postinstall:husky": "husky install",
    "jest-preview": "jest-preview",
    "postinstall:nx": "node ./decorate-angular-cli.js",
    "check:dotnet": "dotnet --version || echo ❌ dotnet CLI must be installed",
    "check:java": "java -version || echo ❌ Java must be installed",
    "affected": "nx affected",
    "affected:apps": "nx print-affected --type=app --select=projects",
    "affected:build": "nx affected:build --configuration=production --parallel",
    "affected:ci": "npm run affected:lint && npm run affected:test && npm run affected:build && npm run affected:e2e -- --configuration=ci",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx print-affected --type=lib --select=projects",
    "affected:lint": "nx affected:lint --max-warnings=0 --parallel",
    "affected:lint:fix": "nx affected:lint --max-warnings=0 --fix --parallel",
    "affected:test": "nx affected:test --parallel",
    "analyze:app-dh": "source-map-explorer dist/apps/dh/app-dh/**/*.js",
    "build": "nx run-many --target=build --configuration=production --all --parallel",
    "ci": "npm run lint && npm run test && npm run build && npm run e2e -- --configuration=ci",
    "designsystem:build": "yarn nx run ui-watt:build-storybook",
    "designsystem:start": "yarn nx run ui-watt:storybook",
    "dep-graph": "nx dep-graph",
    "e2e": "nx run-many --target=e2e --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "help": "nx help",
    "i18n:optimize:app-dh": "transloco-optimize dist/apps/dh/app-dh/assets/i18n",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "lint": "nx workspace-lint && nx run-many --target=lint --max-warnings=0 --all --parallel",
    "lint:fix": "nx workspace-lint && nx run-many --target=lint --max-warnings=0 --fix --all --parallel",
    "ng": "nx",
    "ngcc": "ngcc --properties es2015 module browser main",
    "ngcc:dh": "npm run ngcc -- --use-program-dependencies --tsconfig apps/dh/app-dh/tsconfig.app.json",
    "ngcc:eo": "npm run ngcc -- --use-program-dependencies --tsconfig apps/eo/app-eo/tsconfig.app.json",
    "nx": "nx",
    "start": "ng serve",
    "test": "nx run-many --target=test --all --parallel",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
